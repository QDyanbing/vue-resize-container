{
  "*.{js,ts,jsx,tsx}": [
    "eslint --max-warnings=0 --fix",
    "prettier --cache --write"
  ],
  "*.vue": [
    "eslint --max-warnings=0 --fix",
    "stylelint --fix",
    "prettier --cache --write"
  ],
  "*.{css,scss,less}": [
    "stylelint --fix",
    "prettier --cache --write"
  ],
  "*.{json,md,yaml,yml}": [
    "prettier --cache --write"
  ]
}

